      <div class="popup-content"></div>
    </div>
    <div class="container">
    <%= render 'layouts/flash_messages' %>
    <%= yield %>
    </div>
    <%= render 'layouts/footer' %>
    </div>
    <a href="#" class="back-to-top">â–²</a>
