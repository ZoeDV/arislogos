<ul class="media-list">
  <% posts.each do |post| %>
    <li class="media">
      <div class="media-body">
        <div>
          <span class="text-muted">posted at <%= post.created_at %> by <%= post.user.name %></span>
        </div>
        <img src="..." alt="">
        <div class="text-center">
          <%= post.content%>
        </div>
        <div class="text-right">
          <span class="badge badge-pill">いいね</span>
          <span class="badge badge-pill">違うね</span>
        </div>
      
      </div>
    </li>
  <% end %>
  
  <%= paginate posts %>
</ul>